{
  "name": "template-ts-module",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/satoruk/template-ts-module",
  "keywords": [
    "typescript"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "yarn clean:report && yarn clean:build",
    "clean:build": "del '*.d.ts' '*.js' '!*.config.js'",
    "clean:report": "del coverage '*.log'",
    "commit": "git-cz",
    "lint": "eslint",
    "format": "eslint --fix . && prettier --write .",
    "prepack": "yarn prepare",
    "prepare": "yarn clean && yarn build",
    "release": "semantic-release",
    "test": "jest",
    "test:inspect": "npx --node-arg --inspect-brk jest --runInBand --coverage=false"
  },
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/lodash": ">=4.14.157",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "del-cli": "3.0.1",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "lodash": ">=4.17.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
